/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

.material-symbols-rounded {
	font-variation-settings: 'FILL' 0, 'wght' 400, 'GRAD' 0, 'opsz' 48;

	&.icon-sm {
		font-variation-settings: 'opsz' 32;

		font-size: 1rem;
	}

	&.icon-fill {
		font-variation-settings: 'FILL' 1;
	}
}
